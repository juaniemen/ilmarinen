<p id="notice"><%= notice %></p>

<h1>Listing Count Cps</h1>

<table>
  <thead>
    <tr>
      <th>Cp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @count_cps.each do |count_cp| %>
      <tr>
        <td><%= count_cp.cp %></td>
        <td><%= link_to 'Show', count_cp %></td>
        <td><%= link_to 'Edit', edit_count_cp_path(count_cp) %></td>
        <td><%= link_to 'Destroy', count_cp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Count cp', new_count_cp_path %>
