<p id="notice"><%= notice %></p>

<h1>Listing General Counts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @general_counts.each do |general_count| %>
      <tr>
        <td><%= link_to 'Show', general_count %></td>
        <td><%= link_to 'Edit', edit_general_count_path(general_count) %></td>
        <td><%= link_to 'Destroy', general_count, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New General count', new_general_count_path %>
